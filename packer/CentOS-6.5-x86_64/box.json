{
  "builders": [
    {
      "boot_command": [
        "<esc><wait>",
        "linux ks=http://{{ .HTTPIP }}:{{ .HTTPPort }}/ks.cfg",
        "<enter><wait>"
      ],
      "boot_wait": "10s",
      "guest_os_type": "RedHat_64",
      "headless": true,
      "http_directory": "../",
      "iso_checksum": "f9d84907d77df62017944cb23cab66305e94ee6ae6c1126415b81cc5e999bdd0",
      "iso_checksum_type": "sha256",
      "iso_url": "http://ftp.riken.jp/Linux/centos/6/isos/x86_64/CentOS-6.5-x86_64-minimal.iso",
      "shutdown_command": "sudo shutdown -P now",
      "ssh_password": "vagrant",
      "ssh_username": "vagrant",
      "ssh_wait_timeout": "3000s",
      "type": "virtualbox-iso",
      "vm_name": "box"
    }
  ],
  "post-processors": [
    {
      "output": "CentOS-6.5-x86_64.box",
      "type": "vagrant"
    }
  ],
  "provisioners": [
    {
      "execute_command": "sudo sh '{{ .Path }}'",
      "scripts": [
        "../postinstall/base.sh",
        "../postinstall/puppet.sh",
        "../postinstall/chef.sh",
        "../postinstall/vagrant.sh",
        "../postinstall/virtualbox.sh"
      ],
      "type": "shell"
    }
  ]
}
